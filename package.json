{
  "name": "skilltree",
  "version": "0.6.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessicawestbrook/skilltree.git"
  },
  "bugs": {
    "url": "https://github.com/jessicawestbrook/skilltree/issues"
  },
  "homepage": "https://github.com/jessicawestbrook/skilltree#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:standalone": "next build && cp -r .next/standalone ./standalone",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fix-permissions": "node restore-permissions.js",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test --reporter=github",
    "test:all": "npm run test && npm run test:e2e",
    "test:turbopack": "node scripts/test-turbopack.js",
    "test:pwa": "node scripts/test-pwa.js",
    "generate:icons": "node scripts/generate-pwa-icons.js",
    "migrate:questions": "node scripts/migrate-questions.js",
    "migrate:knowledge": "node scripts/migrate-knowledge-graph.js",
    "migrate:paths": "node scripts/migrate-learning-paths.js",
    "migrate:content": "node scripts/migrate-course-content.js",
    "migrate:progress": "node scripts/migrate-progress-functions.js",
    "migrate:analytics": "node scripts/migrate-analytics.js",
    "migrate:all": "npm run migrate:knowledge && npm run migrate:paths && npm run migrate:questions && npm run migrate:content && npm run migrate:progress && npm run migrate:analytics",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "db:migrate:up": "tsx prisma/migrations/migration-runner.ts up",
    "db:migrate:down": "tsx prisma/migrations/migration-runner.ts down",
    "db:migrate:status": "tsx prisma/migrations/migration-runner.ts status",
    "db:migrate:generate": "tsx prisma/migrations/migration-runner.ts generate",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "lighthouse": "lhci autorun",
    "docker:build": "docker build -t skilltree .",
    "docker:run": "docker run -p 3000:3000 skilltree",
    "ci:test": "npm run lint && npm run typecheck && npm run test:ci"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@opentelemetry/winston-transport": "^0.14.1",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@sentry/nextjs": "^10.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-pwa": "^5.6.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.8.1",
    "resend": "^6.0.1",
    "three": "^0.179.1",
    "web-push": "^3.6.7",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@prisma/client": "^6.14.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
